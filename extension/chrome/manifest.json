{
  "manifest_version": 2,

  "name": "Repo Split",
  "description": "Browse code in a comfortable way",
  "version": "0.2.0",
  "browser_action": {
   "default_title": "Repo Split"
  },
  "icons":{
    "16":"icon.png",
    "48":"icon.png"
  },
  "permissions": [
    "https://github.com/*",
    "https://gitlab.com/*"
  ],
  "background": {
    "scripts": ["starter.js"]
  }
}
